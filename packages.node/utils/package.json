{
  "name": "@mvbri7eo/node-utils",
  "version": "0.0.0",
  "description": "ðŸš€ The package offer node-utils for mvbri7eo!",
  "type": "module",
  "keywords": [
    "monorepo",
    "pnpm",
    "turborepo",
    "changesets",
    "vite",
    "vue",
    "node-utils"
  ],
  "license": "MIT",
  "author": "i7eo",
  "files": [
    "*.d.ts",
    "dist"
  ],
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "dev": "./src/index.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    },
    "./*": [
      "./*",
      "./*.d.ts"
    ]
  },
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./*"
      ]
    }
  },
  "scripts": {
    "build": "tsup --config ./build.config.ts",
    "dev": "DEV=true pnpm build",
    "test": "vitest",
    "lint": "run-s \"lint:**\"",
    "lint:eslint": "TIMING=1 eslint \"*.{js,jsx,ts,tsx,md,json,jsonc}\" --fix",
    "lint:prettier": "prettier --write \"*.{js,ts,jsx,tsx,md,json,jsonc}\"",
    "clean": "run-p \"clean:**\"",
    "clean:cache": "rimraf .turbo && rimraf dist",
    "clean:deps": "rimraf node_modules"
  },
  "dependencies": {
    "find-up": "^7.0.0"
  },
  "devDependencies": {
    "@mvbri7eo/ts-config": "workspace:*"
  },
  "engines": {
    "node": ">=20",
    "pnpm": ">=9"
  }
}
